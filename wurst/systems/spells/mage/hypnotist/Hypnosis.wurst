package Hypnosis
import RegisterEvents

@configurable constant int ABILITY_ID = 'A04Y'

function onCast()
    var target = GetSpellTargetUnit()
    UnitDamageTarget(GetSpellAbilityUnit(), target, 20, false, false, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_UNIVERSAL, null)
    target.addMana(75)

init
    registerSpellEffectEvent(ABILITY_ID, () -> onCast())
